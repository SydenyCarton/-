<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText

        android:id="@+id/edtWpt"
        android:layout_width="223dp"
        android:layout_height="38dp"
        android:hint="edtWpt"
        app:layout_constraintBottom_toTopOf="@+id/edtHpt"
        app:layout_constraintEnd_toEndOf="@+id/edtHpt"
        app:layout_constraintStart_toStartOf="@+id/edtHpt" />

    <EditText

        android:id="@+id/edtHpt"
        android:layout_width="223dp"
        android:layout_height="38dp"
        android:hint="edtHpt"
        app:layout_constraintBottom_toTopOf="@+id/txtHP"
        app:layout_constraintEnd_toEndOf="@+id/txtHP"
        app:layout_constraintStart_toStartOf="@+id/txtHP" />

    <TextView
        android:id="@+id/txtHP"
        android:layout_width="223dp"
        android:layout_height="43dp"
        app:layout_constraintBottom_toTopOf="@+id/txtPrompt"
        app:layout_constraintEnd_toEndOf="@+id/txtPrompt"
        app:layout_constraintStart_toStartOf="@+id/txtPrompt" />

    <TextView
        android:id="@+id/txtPrompt"
        android:layout_width="223dp"
        android:layout_height="38dp"
        android:layout_marginBottom="100dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toTopOf="@+id/btnCancel"
        app:layout_constraintEnd_toEndOf="@+id/btnCancel"
        app:layout_constraintStart_toStartOf="@+id/btnCancel" />

    <Button
        android:id="@+id/btnTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="btnTest"
        app:layout_constraintEnd_toStartOf="@+id/btnCancel"
        app:layout_constraintTop_toBottomOf="@+id/txtPrompt" />

    <Button
        android:id="@+id/btnTest2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="DangerBmi"
        app:layout_constraintStart_toEndOf="@+id/btnCancel"
        app:layout_constraintTop_toBottomOf="@+id/txtPrompt" />

    <Button
        android:id="@+id/btnCancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="btnCancel"
        app:layout_constraintBottom_toTopOf="@+id/linerLayout2"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7" />

    <LinearLayout
        android:id="@+id/linerLayout2"
        android:layout_width="368dp"
        android:layout_height="53dp"
        android:layout_marginBottom="120dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"></LinearLayout>

    <View
        android:id="@+id/view7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgReturn"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="32dp"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:srcCompat="@android:color/holo_red_dark" />

    <ImageView
        android:id="@+id/backHome"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:srcCompat="@android:color/background_dark" />

    <TextView
        android:id="@+id/txtTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="@+id/view7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view7" />
</android.support.constraint.ConstraintLayout>